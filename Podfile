# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

source 'https://github.com/CocoaPods/Specs.git'

target 'Map_Geofencing' do
  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for Map_Geofencing

  pod 'GoogleMaps'
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  
  pod 'GoogleSignIn'
  
  pod 'FacebookCore'
  pod 'FacebookLogin'

  pod 'FBSDKLoginKit'
  pod 'FBSDKCoreKit'
  pod 'FBSDKShareKit'

  pod 'Contentful'

  target 'Map_GeofencingTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'Map_GeofencingUITests' do
    inherit! :search_paths
    # Pods for testing
  end

end
